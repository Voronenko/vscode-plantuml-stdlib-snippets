{
  "name": "vscode-plantuml-stdlib-snippets",
  "displayName": "PlantUML stdlib snippets",
  "description": "Simple extensions for quickier dealing with PlantUML stdlib library",
  "version": "0.2.0",
  "publisher": "voronenko",
  "icon": "images/logo.png",
  "keywords": [
    "snippets",
    "plantuml",
    "aws-plantuml",
    "awslib-icons-for-plantuml",
    "azure-plantuml",
    "cloudinsight-plantuml",
    "cloudogu-plantuml",
    "elastic-plantuml",
    "logos-plantuml",
    "material-plantuml",
    "office-plantuml",
    "osa-plantuml",
    "tupadr3-plantuml"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Voronenko/vscode-plantuml-stdlib-snippets.git"
  },
  "engines": {
    "vscode": "^1.22.0"
  },
  "categories": [
    "Snippets"
  ],
  "extensionKind": [
     "workspace",
     "ui"
  ],
  "contributes": {
  "languages": [
    {
      "id": "diagram",
      "extensions": [
 	".wsd",
	".pu",
	".puml",
        ".plantuml",
        ".iuml"
      ],
      "aliases": [
 	"Diagram",
	"PlantUML",
        "plantuml"
      ],
      "configuration": "./language-configuration.json"
    }
    ],
    "snippets": [
      {
        "language": "diagram",
        "path": "./snippets/plantuml-awslib-icons-for-plantuml.code-snippets"
      },
      {
        "language": "diagram",
        "path": "./snippets/plantuml-azure-icons-for-plantuml.code-snippets"
      },
      {
